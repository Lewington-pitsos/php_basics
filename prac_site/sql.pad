CREATE TABLE people (
  id INTEGER NOT NULL AUTO_INCREMENT,
  name CHAR(30) NOT NULL,
  age INTEGER NOT NULL DEFAULT 30,
  happy BOOL NOT NULL DEFAULT false,
  PRIMARY KEY(id)
);


